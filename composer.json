{
    "name": "do-while/contao-pdf-gateway-bundle",
    "description": "A Notification Center gateway for Contao that generates a pdf file from SimpleTokens and attaches it to notifications.",
    "keywords": ["contao", "pdf", "gateway", "notification-center"],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [{
        "name": "Softleister - Hagen Klemp",
        "homepage": "https://www.softleister.de",
        "role": "Developer"
    }],
    "support": {
        "issues": "https://github.com/do-while/contao-pdf-gateway-bundle/issues",
        "source": "https://github.com/do-while/contao-pdf-gateway-bundle"
    },
    "require": {
        "php": "^8.2",
        "contao/core-bundle": "^5.3",
        "menatwork/contao-multicolumnwizard-bundle": "^3.6",
        "terminal42/notification_center": "^2.0.5",
        "codefog/contao-haste":"^5.0",
        "tecnickcom/tcpdf": "^6.6",
        "setasign/fpdi": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Softleister\\ContaoPdfGatewayBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Softleister\\ContaoPdfGatewayBundle\\ContaoManager\\Plugin"
    }
}
